	#enableMic: "captures sound from mic if its set to true",
	enableMic: false,
	
	manageNetwork: 1,

	#reconnectDuration: "time to reconnect in minutes when network disconnected",
	reconnectDuration: 2,
	
	reconnectPollCount: 2,

	#mobileBroadbandConnection: "if u use wvdial to connect set it to 'wvdial' without quotes or if u use network manager set it to connection name. If connection name contains spaces, put it in double quotes",
	mobileBroadbandConnection: "wvdial",

	#configModem: "to run wvdialconf at startup put it true default false",
	configModem: false,

	#mobileModemVendorProductId: "colon separated vendor id and product id of mobile modem",
	mobileModemVendorProductId: "19d2:fff1",

	#usbHubVendorProductId: "colon separated vendor id and product id of mobile modem",
	usbHubVendorProductId: "05e3:0608",

