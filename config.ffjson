	serverAddress: "demo.viewyourworld.com",
	
	serverPort: 80,
	
	streamAddress: "demo.viewyourworld.com",
	
	streamPort: 25222,

	#serverType: "REST/SOAP",
	serverType: "SOAP",

	#connectionEncryption: "NO-default value,TLS1_1-TLS version 1.1 encryption",
