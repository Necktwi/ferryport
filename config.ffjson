	#debug: "
		debug value in context of 
		basic=1,
		spawn=2,
		socket=4,
		gps=8,
		MediaManager=16,
		low-level calls like signalHandler, spawn = 32,
		pcm=64,
		video capture=128,
		video capture low level or frequent log = 256,
		libavencoder=512,
		MediaManagerDeep=1024
	",
	debug: 1169,

	#cmosBatt: "true if main-board has CMOS battery else false",
	cmosBatt: false,
	
	systemId: 1780,

	#videoStreamingType: "
		for ffmpeg and flash set to 'RTMP';
		for FairPlay set to 'FMSP'
	",
	videoStreamingType: "RTMP",
	
	autoInsertCameras: true,
	pollInterval: 60000,
	pid: 4188
